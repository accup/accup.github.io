{
  "name": "@accup/vue-search-params",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "author": "Accup",
  "description": "Reactive search parameters for Vue.js.",
  "main": "./dist/vue-search-params.umd.cjs",
  "module": "./dist/vue-search-params.js",
  "types": "./dist/vue-search-params.d.ts",
  "exports": {
    ".": {
      "types": "./dist/vue-search-params.d.ts",
      "import": "./dist/vue-search-params.js",
      "require": "./dist/vue-search-params.umd.cjs"
    }
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/accup/accup.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/accup/accup.github.io/issues"
  },
  "homepage": "https://github.com/accup/accup.github.io#readme",
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:js": "npm run build:js -- --watch",
    "dev:ts": "npm run build:ts -- --watch",
    "dev:dts": "npm run build:dts -- --watch",
    "build": "npm run build:js && npm run build:ts && npm run build:dts",
    "build:js": "rollup --config rollup.config.ts --configPlugin typescript",
    "build:ts": "rollup --config rollup-ts.config.ts --configPlugin typescript",
    "build:dts": "rollup --config rollup-dts.config.ts --configPlugin typescript"
  }
}
