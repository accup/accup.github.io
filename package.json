{
  "name": "accup.github.io",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Accup",
  "scripts": {
    "dev": "node ./bin/dev.js",
    "build": "npm run build --workspaces --if-present && npm run build:sole",
    "dev:sole": "vite",
    "build:sole": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:prettier": "prettier . --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier -- --check",
    "fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write"
  },
  "dependencies": {
    "@accup/satis": "^0.0.2",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/vite-plugin": "^3.6.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.9"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.7.0",
    "prettier": "^2.7.1"
  }
}
