{
  "name": "accup.github.io",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": "Accup",
  "scripts": {
    "dev": "node ./bin/dev.js",
    "build": "npm run build --workspaces && npm run build:self",
    "dev:self": "vite",
    "build:self": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:prettier": "prettier . --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier -- --check",
    "fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write"
  },
  "dependencies": {
    "@accup/it": "^0.0.0",
    "@accup/util-rollup": "^0.0.0",
    "@accup/vue-search-params": "^0.0.0",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/vite-plugin": "^3.6.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vue": "^3.2.41",
    "vue-tsc": "^1.0.9"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "prettier": "^2.7.1",
    "rollup-plugin-dts": "^5.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
